{
    "ts-node": {
        "compilerOptions": {
          "module": "commonjs"
        }
    },
    "compilerOptions": {
        "target": "es6",
        "module": "commonjs",
        "types": ["node", "jest"],
        "strict": true,
        "forceConsistentCasingInFileNames": true
